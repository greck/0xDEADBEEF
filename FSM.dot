digraph FSM {

"START" [ shape=circle label="start" ];
START -> QUIESCE;

"QUIESCE" [ shape=box label=<<table border="0"><tr><td align="left">QUIESCE</td></tr><tr><td align="left">r=Quiesce</td></tr></table>> ]:
QUIESCE -> QTOUCHTESTUP1 [ label = "T/F" ];

"QTOUCHTESTUP1" [ shape=box label=<<table border="0"><tr><td align="left">QTOUCHTESTUP1</td></tr><tr><td align="left">r=IsNotTouched</td></tr></table>> ]:
QTOUCHTESTUP1 -> QTOUCHTESTDOWN [ label = "T" ];
QTOUCHTESTUP1 -> QTOUCHTESTUP1 [ label = "F" ];

"QTOUCHTESTDOWN" [ shape=box label=<<table border="0"><tr><td align="left">QTOUCHTESTDOWN</td></tr><tr><td align="left">r=IsTouched</td></tr></table>> ]:
QTOUCHTESTDOWN -> QTOUCHTESTUP2 [ label = "T" ];
QTOUCHTESTDOWN -> QTOUCHTESTDOWN [ label = "F" ];

"QTOUCHTESTUP2" [ shape=box label=<<table border="0"><tr><td align="left">QTOUCHTESTUP2</td></tr><tr><td align="left">r=IsNotTouched</td></tr></table>> ]:
QTOUCHTESTUP2 -> TOUCHTESTDOWN [ label = "T" ];
QTOUCHTESTUP2 -> QTOUCHTESTUP2 [ label = "F" ];

"TOUCHTESTDOWN" [ shape=box label=<<table border="0"><tr><td align="left">TOUCHTESTDOWN</td></tr><tr><td align="left">r=IsTouched</td></tr></table>> ]:
TOUCHTESTDOWN -> QUIESCE [ label = "T" ];
TOUCHTESTDOWN -> DETECTBALL [ label = "F" ];

"DETECTBALL" [ shape=box label=<<table border="0"><tr><td align="left">DETECTBALL</td></tr><tr><td align="left">r=DetectBall</td></tr></table>> ]:
DETECTBALL -> AVOIDBALL [ label = "T" ];
DETECTBALL -> BOUNDARYCHECK [ label = "F" ];

"AVOIDBALL" [ shape=box label=<<table border="0"><tr><td align="left">AVOIDBALL</td></tr><tr><td align="left">r=AvoidBall</td></tr></table>> ]:
AVOIDBALL -> BOUNDARYCHECK [ label = "T/F" ];

"BOUNDARYCHECK" [ shape=box label=<<table border="0"><tr><td align="left">BOUNDARYCHECK</td></tr><tr><td align="left">r=LineTest</td></tr></table>> ]:
BOUNDARYCHECK -> SWEEP [ label = "T" ];
BOUNDARYCHECK -> STRAIGHT [ label = "F" ];

"STRAIGHT" [ shape=box label=<<table border="0"><tr><td align="left">STRAIGHT</td></tr><tr><td align="left">r=Straight</td></tr></table>> ]:
STRAIGHT -> TOUCHTESTDOWN [ label = "T/F" ];

"SWEEP" [ shape=box label=<<table border="0"><tr><td align="left">SWEEP</td></tr><tr><td align="left">r=Sweep</td></tr></table>> ]:
SWEEP -> TOUCHTESTDOWN [ label = "T/F" ];

}